SI FORMATO=501
    FINICIAL=FENGACT
    SUELDONUEVO=@VARFEC('SMLV',FINICIAL)/2
    SUELDONUEVO=@REDON(SUELDONUEVO)
    CARGOPUESTO='1027'
    SI CARGO<>'1027'
        ESTADO=3
        FINAL
    FINSI
FINSI
SI FORMATO=502
    FINICIAL=FENGACT
    SUELDONUEVO=@VARFEC('SMLV',FINICIAL)
    CARGOPUESTO='1028'
    SI CARGO<>'1028'
        ESTADO=3
        FINAL
    FINSI
FINSI
SI FORMATO EN (99,101)
    FINICIAL=FENGACT
    SI FORMATO<>99
        SI SUELDONUEVO<=0
             ESTADO=35
             FINAL
        FINSI
       SI VINCULACION<>1
           ESTADO=117
       FINSI
    FINSI
    SUELDO=SUELDONUEVO
FINSI
SI FORMATO=201
     SI VINCULACION<>2
        ESTADO=117
     FINSI
     FINICIAL=FENGACT
      SI SUELDONUEVO<=0
         ESTADO=35
         FINAL
    FINSI
    SUELDO=SUELDONUEVO
FINSI

SI FORMATO EN (301,302)
    FINICIAL=FENGACT
    SI SUELDONUEVO<=0
         ESTADO=35
         FINAL
    FINSI
    SI VINCULACION<>3
        ESTADO=117
     FINSI
    SUELDO=SUELDONUEVO
FINSI
SI FORMATO=302
   DIAS=@DIFFECHA(FFINAL,FINICIAL)
   SI DIAS>=360
       ESTADO=6
       FINAL
   FINSI
FINSI
SI FORMATO=503
    FINICIAL=FENGACT
    SUELDONUEVO=@VARFEC('ASEP',FINICIAL)
    CARGOPUESTO='1029'
    SI CARGO<>'1026'
        ESTADO=3
        FINAL
    FINSI
FINSI
SI FORMATO EN (501,502,503)
    SI EMPRESA=1 Y TIPOPERIODO<>1
        ESTADO=5
        FINAL
    FINSI
    SI EMPRESA<>1 Y TIPOPERIODO<>4
        ESTADO=5
        FINAL
    FINSI
FINSI
SI FORMATO=9
    LINEA=LINEAPUESTO
FINSI
SI FORMATO=12
    FTERMINA=FFINAL
    FHASTA=FFINAL
FINSI
